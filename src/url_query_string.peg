query_string <- first_param second_param*
`
[H, T] = Node,
[H | T]
`;

first_param <- '?' param
`
[_, Param] = Node,
Param
`;

second_param <- '&' param
`
[_, Param] = Node,
Param
`;

param <- name:word '=' value:word
`
[{name, Name}, _, {value, Value}] = Node,
{binary_to_atom(Name, latin1), Value}
`;

word <- char*
`iolist_to_binary(Node)`;

char <- [^?&#=];